# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for OAuth redirects)
FRONTEND_URL=http://localhost:5173

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Calendar API (for future use)
GOOGLE_CLIENT_ID=your_firebase_web_client_id
GOOGLE_CLIENT_SECRET=your_firebase_web_client_secret
GOOGLE_REDIRECT_URI=http://localhost:5000/api/auth/google/callback

# Email Service (for daily digest)
SENDGRID_API_KEY=your_sendgrid_api_key

# Google AI (Gemini) Configuration
GOOGLE_AI_API_KEY=your_gemini_api_key 

# Firebase Configuration (for mobile Google auth)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY_ID=your_firebase_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour Firebase Private Key Here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_firebase_client_email@your_project.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_firebase_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your_firebase_client_email%40your_project.iam.gserviceaccount.com

# Google OAuth client IDs (used for verifying Google Sign-In ID tokens)
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id.apps.googleusercontent.com

FEEDBACK_EMAIL_USER=your_gmail_address@gmail.com
FEEDBACK_EMAIL_PASS=your_gmail_app_password
FEEDBACK_EMAIL_TO=your_destination_email@gmail.com
EMAIL_SERVICE=gmail 