# Server Configuration
PORT=5000
NODE_ENV=production

# Railway Ingress Configuration (REQUIRED for production)
# Comma-separated list of CIDR blocks for Railway's ingress proxies
# This MUST be configured in production to trust Railway's proxy infrastructure
# Contact Railway support or check their documentation for current ingress IP ranges
# Example: "198.51.100.0/24,203.0.113.0/24"
RAILWAY_INGRESS_CIDR=
# Frontend URL (for OAuth redirects)
FRONTEND_URL=https://your-frontend.example.com
CORS_ORIGIN=https://your-frontend.example.com

# Supabase Configuration (PROD project)
SUPABASE_URL=your_prod_supabase_project_url
SUPABASE_ANON_KEY=your_prod_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_prod_supabase_service_role_key
# Note: SUPABASE_JWT_SECRET is no longer needed when using JWT Signing Keys.
# The Supabase SDK automatically fetches public keys via JWKS (JSON Web Key Set).
# If you're still using Legacy HS256 JWT Secret, you can set SUPABASE_JWT_SECRET,
# but it's recommended to migrate to JWT Signing Keys (ECC P-256) in Supabase dashboard.

# Google OAuth (PROD)
GOOGLE_CLIENT_ID=your_prod_google_client_id
GOOGLE_CLIENT_SECRET=your_prod_google_client_secret
GOOGLE_REDIRECT_URI=https://your-frontend.example.com/auth/callback

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key

# Google AI (Gemini)
GOOGLE_AI_API_KEY=your_prod_gemini_api_key

# Token Encryption Key (32+ characters)
ENCRYPTION_KEY=your_32_character_encryption_key_here

FEEDBACK_EMAIL_USER=your_gmail_address@gmail.com
FEEDBACK_EMAIL_PASS=your_gmail_app_password
FEEDBACK_EMAIL_TO=your_destination_email@gmail.com
EMAIL_SERVICE=gmail



